<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>相册</title>
    <script src="/js/jquery.min.js"></script>
    <script src="/js/jquery.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <!-- Bootstrap core CSS -->
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/style.css" rel="stylesheet">
    <script src="/js/html5shiv.min.js"></script>
    <script src="/js/respond.min.js"></script>
    <script src="js/act.js"></script>
</head>
<body>
<%include head.ejs%>
<div class="container" style="padding-top: 50px">
    <form action="#" method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label for="exampleInputFile">上传照片</label>
            <input type="file" name="imgSrc" value="">
        </div>
        <div class="form-group">
            <label for="exampleInputFile">选择相册</label>
            <select name="dirname" value="" class="form-control">
                <% for (var i=0;i<photodir.length;i++){%>
                <option><%=photodir[i].pname%></option>
                <%}%>
            </select>
        </div>
        <button type="submit" class="btn btn-default">上传</button>
    </form>
</div>
</body>
</html>
<script src="/js/bootstrap.min.js"></script>
